{
  "version": 1,
  "projects": {
    "landingpage": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/landingpage",
      "sourceRoot": "apps/landingpage/src",
      "prefix": "dev-blog",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/landingpage",
            "index": "apps/landingpage/src/index.html",
            "main": "apps/landingpage/src/main.ts",
            "polyfills": "apps/landingpage/src/polyfills.ts",
            "tsConfig": "apps/landingpage/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/landingpage/src/favicon.ico",
              "apps/landingpage/src/assets"
            ],
            "styles": ["apps/landingpage/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/landingpage/src/environments/environment.ts",
                  "with": "apps/landingpage/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "landingpage:build",
            "proxyConfig": "apps/landingpage/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "landingpage:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "landingpage:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/landingpage/tsconfig.app.json",
              "apps/landingpage/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/landingpage/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/landingpage/jest.config.js",
            "tsConfig": "apps/landingpage/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/landingpage/src/test-setup.ts"
          }
        }
      }
    },
    "landingpage-e2e": {
      "root": "apps/landingpage-e2e",
      "sourceRoot": "apps/landingpage-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/landingpage-e2e/cypress.json",
            "tsConfig": "apps/landingpage-e2e/tsconfig.e2e.json",
            "devServerTarget": "landingpage:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "landingpage:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/landingpage-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/landingpage-e2e/**"]
          }
        }
      }
    },
